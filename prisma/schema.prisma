// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:123qwe@localhost:5435"
}

model user_login{
    id Int @id @default(autoincrement())
    status Boolean
    name String
}

model books{
    id Int @id @default(autoincrement())
    status Boolean
    title String
    author String
}